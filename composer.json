{
	"name" : "The Experiment LLC/Brighten",
	"description" : "Brighten Project",
	"keywords" : [
		"brighten",
		"sew"
	],
	"license" : "proprietary",
	"require" : {
		"php" : ">=5.5.15",
		"laravel/framework" : "4.2.*"
	},
	"require-dev" : {
		"heroku/heroku-buildpack-php" : "*"
	},
	"autoload" : {
		"classmap" : [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts" : {
		"post-install-cmd" : [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd" : [
			"php artisan clear-compiled",
			"php artisan optimize"
		]
	},
	"config" : {
		"preferred-install" : "dist"
	},
	"minimum-stability" : "stable",
	"authors" : [{
			"name" : "Nick Velloff",
			"email" : "nickvelloff@theexperiment.io",
